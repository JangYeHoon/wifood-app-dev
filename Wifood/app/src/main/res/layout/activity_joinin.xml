<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.Joinin">

    <EditText
        android:id="@+id/editTextJoinEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="xxx@xxx.com"
        android:inputType="textEmailAddress"
        android:minHeight="48dp"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="35dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이메일"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="47dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="비밀번호"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="94dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="비밀번호 확인"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="174dp" />

    <EditText
        android:id="@+id/editTextJoinPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="특수문자 2개 포함 8~15자"
        android:inputType="textPassword"
        android:minHeight="48dp"
        android:password="true"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="90dp" />

    <EditText
        android:id="@+id/editTextJoinPasswordCheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="비밀번호 동일 입력"
        android:inputType="textPassword"
        android:minHeight="48dp"
        android:password="true"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="158dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="닉네임"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="248dp" />

    <EditText
        android:id="@+id/editTextJoinNickName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="한글,영어,숫자만 입력"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="232dp" />

    <EditText
        android:id="@+id/editTextJoinPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="'-'을 빼고 입력"
        android:inputType="phone"
        android:minHeight="48dp"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="301dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="핸드폰 번호"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="317dp" />

    <EditText
        android:id="@+id/editTextJoinFineAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="상세 주소를 입력하세요"
        android:inputType="textPostalAddress"
        android:minHeight="48dp"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="488dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="주소"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="387dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="상세주소"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="504dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="회원가입"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="10dp" />

    <Button
        android:id="@+id/btnGoJoinInFoodInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:enabled="true"
        android:text="완료"
        tools:layout_editor_absoluteX="177dp"
        tools:layout_editor_absoluteY="658dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="성별"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="579dp" />

    <RadioGroup
        android:id="@+id/radioGroupJoinGender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="566dp">

        <RadioButton
            android:id="@+id/radioButtonJoinMale"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Male" />

        <RadioButton
            android:id="@+id/radioButtonJoinFemale"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Femail" />
    </RadioGroup>

    <Button
        android:id="@+id/btnCoarseAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="주소"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="373dp" />

    <EditText
        android:id="@+id/editTextJoinCoarseAddress"
        android:layout_width="339dp"
        android:layout_height="43dp"
        android:background="@color/cardview_light_background"
        android:clickable="false"
        android:ems="10"
        android:hint="주소 버튼을 눌러주세요"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="428dp" />

</androidx.constraintlayout.widget.ConstraintLayout>