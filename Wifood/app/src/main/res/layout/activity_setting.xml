<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="360dp"
    android:layout_height="640dp"
    android:background="@color/white">

    <ImageButton
        android:id="@+id/imageButton_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="20dp"
        android:background="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_button" />

    <TextView
        android:id="@+id/textView_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/pbold"
        android:text="설정"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_top"
        android:layout_width="0dp"
        android:layout_height="2px"
        android:layout_marginTop="48dp"
        android:background="@color/enable"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_appVersion"
        android:layout_width="360dp"
        android:layout_height="45dp"
        android:layout_marginTop="48dp"
        android:background="@color/white"
        android:backgroundTint="@color/white"
        android:fontFamily="@font/rmedium"
        android:paddingLeft="15dp"
        android:text="앱 버젼"
        android:textAlignment="textStart"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_feedBack"
        android:layout_width="360dp"
        android:layout_height="45dp"
        android:background="@color/white"
        android:backgroundTint="@color/white"
        android:drawableRight="@drawable/go_button"
        android:paddingRight="15dp"
        android:fontFamily="@font/rmedium"
        android:paddingLeft="15dp"
        android:text="피드백"
        android:textAlignment="textStart"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_appVersion" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_devInfo"
        android:layout_width="360dp"
        android:layout_height="45dp"
        android:background="@color/white"
        android:backgroundTint="@color/white"
        android:drawableRight="@drawable/go_button"
        android:fontFamily="@font/rmedium"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:text="개발자 정보"
        android:textAlignment="textStart"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_feedBack" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_serviceInfo"
        android:layout_width="360dp"
        android:layout_height="45dp"
        android:background="@color/white"
        android:backgroundTint="@color/white"
        android:fontFamily="@font/rmedium"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:drawableRight="@drawable/go_button" android:shadowColor="@color/white"
        android:text="서비스 이용약관"
        android:textAlignment="textStart"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_devInfo" />

</androidx.constraintlayout.widget.ConstraintLayout>